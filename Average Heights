import array as arr
a = arr.array('i', [100, 98, 200, 120, 101, 190, 109, 122, 133, 153])
n = 10


print("the total heights are ", (a[0]+a[1]+a[2]+a[3]+a[4]+a[5]+a[6]+a[7]+a[8]+a[9]))


print("the average height is ", (a[0]+a[1]+a[2]+a[3]+a[4]+a[5]+a[6]+a[7]+a[8]+a[9])/n)


